(use-modules (ice-9 rdelim))
(use-modules (ice-9 poe))
(define (stairnacci n)
  (if (< n 4)
    n
    (+ (funcq-stair (1- n)) (funcq-stair (- n 2)))))
(define funcq-stair (pure-funcq stairnacci))
(define input (open-input-file (list-ref (command-line) 1)))
(let loop ((line (read-line input)))
  (cond ((not (eof-object? line))
    (if (not (string-null? line))
      (begin
        (display (funcq-stair (string->number line)))
        (newline)))
    (loop (read-line input)))))
(close input)
